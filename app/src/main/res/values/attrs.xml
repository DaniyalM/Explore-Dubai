<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="color_dubai_logo" format="color" />
    <attr name="color_dubai_logo_inverse" format="color" />
    <attr name="color_dubai_logo_dark" format="color" />
    <attr name="colorOnBackgroundDark" format="color" />
    <attr name="colorSurfaceInverted" format="color" />
    <attr name="color_light_gray_svg" format="color" />
    <attr name="color_light_white_dark_gray" format="color" />
    <attr name="color_light_white_dark_gray_icon" format="color" />
    <attr name="color_light_white_dark_gray_night" format="color" />
    <attr name="color_edit_text_bg" format="color" />
    <attr name="color_card_views_shadow" format="color" />
    <attr name="color_curve_view_bg" format="color" />
    <attr name="color_curve_login_bg" format="color" />
    <attr name="color_login_edit_text_bg" format="color" />
    <attr name="color_pyt_purple_light" format="color" />
    <attr name="color_pyt_orange_light" format="color" />
    <attr name="color_ambient_shadow" format="color" />

    <attr name="color_light_theme_purple_dark_theme_gray" format="color" />
    <attr name="color_light_theme_dark_theme_gray" format="color" />
    <attr name="color_light_theme_dark_theme_dark_gray" format="color" />
    <attr name="color_light_theme_purple_dark_theme" format="color" />
    <attr name="color_chip_text_theme" format="color" />
    <attr name="colorEventLight" format="color" />
    <attr name="colorSurfaceInvertedGray" format="color" />
    <attr name="colorSecondaryVariantInverted" format="color" />


    <declare-styleable name="CustomTextView">
        <attr name="textFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="HorizontalItemSelector">
        <attr name="selectedTextColor" format="color" />
        <attr name="unSelectedTextColor" format="color" />
        <attr name="selectedBackground" format="reference" />
        <attr name="unSelectedBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="HorizontalAttractionSelector">
        <attr name="selectedTextColor1" format="color" />
        <attr name="unSelectedTextColor1" format="color" />
        <attr name="selectedBackground1" format="reference" />
        <attr name="unSelectedBackground1" format="reference" />

        <attr name="selectedInnerImg" format="reference" />
        <attr name="unSelectedInnerImg" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="textTypeFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FabBottomNavigationView">
        <attr name="fab_cradle_margin" format="dimension"/>
        <attr name="fab_cradle_rounded_corner_radius" format="dimension"/>
        <attr name="cradle_vertical_offset" format="dimension"/>
        <attr name="fab_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="cb_textFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
        <attr name="cb_type" format="enum">
            <enum name="filled" value="0" />
            <enum name="outlined" value="1" />
        </attr>
    </declare-styleable>


    <declare-styleable name="CustomReadMoreView">
        <attr name="trimExpandedText" format="string" />
        <attr name="trimCollapsedText" format="string" />
        <attr name="trimLength" format="integer" />
        <attr name="showTrimExpandedText" format="boolean" />
        <attr name="colorClickableText" format="color" />
        <attr name="trimLines" format="integer" />
        <attr name="trimMode">
            <enum name="trimModeLine" value="0" />
            <enum name="trimModeLength" value="1" />
        </attr>
        <attr name="customReadMoreTextFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomTextInputLayout">
        <attr name="textTypeFace" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="AppWidgetAttrs">
        <attr name="appWidgetBackgroundColor" format="color" />
        <attr name="appWidgetTextColor" format="color" />
    </declare-styleable>

</resources>