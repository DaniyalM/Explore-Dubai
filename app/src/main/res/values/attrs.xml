<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">
        <attr name="textFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>


    <declare-styleable name="HorizontalItemSelector">
        <attr name="selectedTextColor" format="color" />
        <attr name="unSelectedTextColor" format="color" />
        <attr name="selectedBackground" format="reference" />
        <attr name="unSelectedBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="HorizontalAttractionSelector">
        <attr name="selectedTextColor1" format="color" />
        <attr name="unSelectedTextColor1" format="color" />
        <attr name="selectedBackground1" format="reference" />
        <attr name="unSelectedBackground1" format="reference" />

        <attr name="selectedInnerImg" format="reference" />
        <attr name="unSelectedInnerImg" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="textTypeFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FabBottomNavigationView">
        <attr name="fab_cradle_margin" format="dimension"/>
        <attr name="fab_cradle_rounded_corner_radius" format="dimension"/>
        <attr name="cradle_vertical_offset" format="dimension"/>
        <attr name="fab_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="cb_textFont" format="enum">
            <enum name="light" value="0" />
            <enum name="regular" value="1" />
            <enum name="bold" value="2" />
        </attr>
        <attr name="cb_type" format="enum">
            <enum name="filled" value="0" />
            <enum name="outlined" value="1" />
        </attr>
    </declare-styleable>

</resources>