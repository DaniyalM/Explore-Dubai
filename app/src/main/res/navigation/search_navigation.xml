<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/search_navigation"
    app:startDestination="@id/searchFragment">

    <fragment
        android:id="@+id/searchFragment"
        android:name="com.app.dubaiculture.ui.postLogin.search.SearchFragment"
        android:label="SearchFragment">
        <action
            android:id="@+id/action_searchFragment_to_sortFragment"
            app:destination="@id/sortFragment" />
    </fragment>
    <dialog
        android:id="@+id/sortFragment"
        android:name="com.app.dubaiculture.ui.postLogin.nearyou.SortFragment"
        android:label="SortFragment" />
</navigation>