<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pre_login_navigation"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.app.dubaiculture.ui.preLogin.splash.SplashFragment"
        tools:layout="@layout/fragment_splash"
        android:label="fragment_splash">
        <action
            android:id="@+id/action_splashFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_splashFragment_to_onBoardingFragment"
            app:destination="@id/onBoardingFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.app.dubaiculture.ui.preLogin.login.LoginFragment"
        tools:layout="@layout/fragment_login"
        android:label="fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment2"
            app:destination="@id/registerFragment2"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_loginFragment_to_forgotFragment"
            app:destination="@id/forgotFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_loginFragment_to_bottomSheet"
            app:destination="@id/bottomSheet"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_loginFragment_to_createPassFragment"
            app:destination="@id/createPassFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_loginFragment_to_registrationSuccessFragment"
            app:destination="@id/registrationSuccessFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment2"
        tools:layout="@layout/fragment_register"
        android:name="com.app.dubaiculture.ui.preLogin.registeration.RegisterFragment"
        android:label="RegisterFragment" >
        <action
            android:id="@+id/action_registerFragment2_to_bottomSheet"
            app:destination="@id/bottomSheet"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/forgotFragment"
        tools:layout="@layout/fragment_forgot_"
        android:name="com.app.dubaiculture.ui.preLogin.forgot.ForgotFragment"
        android:label="ForgotFragment" >

        <action
            android:id="@+id/action_forgotFragment_to_bottomSheet"
            app:destination="@id/bottomSheet"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/createPassFragment"
        tools:layout="@layout/fragment_create_pass"
        android:name="com.app.dubaiculture.ui.preLogin.password.CreatePassFragment"
        android:label="CreatePassFragment" >
        <action
            android:id="@+id/action_createPassFragment_to_passwordUpdatedFragment"
            app:destination="@id/passwordUpdatedFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/registrationSuccessFragment"
        tools:layout="@layout/fragment_registeration_success"
        android:name="com.app.dubaiculture.ui.preLogin.registeration.RegistrationSuccessFragment"
        android:label="RegistrationSuccessFragment" >
        <action
            app:launchSingleTop="true"
            app:popUpTo="@+id/pre_login_nav_host_fragment"
            app:popUpToInclusive="true"
            android:id="@+id/action_registrationSuccessFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <dialog
        android:id="@+id/bottomSheet"
        android:name="com.app.dubaiculture.ui.preLogin.registeration.otp.OTPFragment"
        tools:layout="@layout/fragment_o_t_p" >
        <argument
            android:name="verificationCode"
            app:argType="string"/>

        <argument
            android:name="screen_name"
            app:argType="string"/>

        <argument
            android:name="emailorphone"
            app:nullable="true"
            android:defaultValue="@null"
            app:argType="string"/>

        <argument
            android:name="password"
            app:nullable="true"
            android:defaultValue="@null"
            app:argType="string"/>


        <action
            android:id="@+id/action_bottomSheet_to_registrationSuccessFragment"
            app:destination="@id/registrationSuccessFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />

        <action
            android:id="@+id/action_bottomSheet_to_createPassFragment"
            app:destination="@id/createPassFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />

    </dialog>
    <dialog
        android:id="@+id/passwordUpdatedFragment"
        android:name="com.app.dubaiculture.ui.preLogin.password.passwordupdated.PasswordUpdatedFragment"
        android:label="PasswordUpdatedFragment"
        tools:layout="@layout/fragment_password_updated"
        >
        <action
            app:launchSingleTop="true"
            app:popUpTo="@+id/pre_login_nav_host_fragment"
            app:popUpToInclusive="true"
            android:id="@+id/action_passwordUpdatedFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </dialog>
    <fragment
        android:id="@+id/onBoardingFragment"
        android:name="com.app.dubaiculture.ui.preLogin.onboarding.OnBoardingFragment"
        android:label="OnBoardingFragment" >
        <action
            android:id="@+id/action_onBoardingFragment_to_onBoardingAFragment"
            app:destination="@id/onBoardingAFragment" />
        <action
            android:id="@+id/action_onBoardingFragment_to_onBoardingBFragment"
            app:destination="@id/onBoardingBFragment" />
        <action
            android:id="@+id/action_onBoardingFragment_to_onBoardingCFragment"
            app:destination="@id/onBoardingCFragment" />
        <action
            android:id="@+id/action_onBoardingFragment_to_onBoardingDFragment"
            app:destination="@id/onBoardingDFragment" />
    </fragment>
    <fragment
        android:id="@+id/onBoardingAFragment"
        android:name="com.app.dubaiculture.ui.preLogin.onboarding.steps.OnBoardingAFragment"
        android:label="OnBoardingAFragment" />
    <fragment
        android:id="@+id/onBoardingBFragment"
        android:name="com.app.dubaiculture.ui.preLogin.onboarding.steps.OnBoardingBFragment"
        android:label="OnBoardingBFragment" />
    <fragment
        android:id="@+id/onBoardingCFragment"
        android:name="com.app.dubaiculture.ui.preLogin.onboarding.steps.OnBoardingCFragment"
        android:label="OnBoardingCFragment" />
    <fragment
        android:id="@+id/onBoardingDFragment"
        android:name="com.app.dubaiculture.ui.preLogin.onboarding.steps.OnBoardingDFragment"
        android:label="OnBoardingDFragment" />

</navigation>