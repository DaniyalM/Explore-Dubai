<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/news_navigation"
    app:startDestination="@id/latestNewsFragment">
    <fragment
        android:id="@+id/latestNewsFragment"
        android:name="com.app.dubaiculture.ui.postLogin.latestnews.LatestNewsFragment"
        android:label="LatestNewsFragment" >
        <argument
            android:name="culture"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_latestNewsFragment_to_newsDetailFragment"
            app:destination="@id/newsDetailFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" >
            <argument
                android:name="news_id"
                app:argType="string"
                app:nullable="false" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/newsDetailFragment"
        android:name="com.app.dubaiculture.ui.postLogin.latestnews.detail.NewsDetailFragment"
        android:label="NewsDetailFragment">
        <argument
            android:name="news_id"
            app:argType="string"
            app:nullable="false" />

    </fragment>


</navigation>