        tools:layout="@layout/fragment_explore_map">
        <action
            android:id="@+id/action_exploreMapFragment_to_exploreBottomSheetFragment"
            app:destination="@id/exploreBottomSheetFragment"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <dialog
        android:id="@+id/exploreBottomSheetFragment"
        android:name="com.app.dubaiculture.ui.postLogin.explore.bottomsheet.ExploreBottomSheetFragment"
        android:label="ExploreBottomSheetFragment"
        tools:layout="@layout/fragment_explore_buttom_sheet" />

    <fragment
        android:id="@+id/eventListingFragment"
        android:name="com.app.dubaiculture.ui.postLogin.events.EventListingFragment"
        android:label="EventListingFragment"
        tools:layout="@layout/fragment_event_listing">
        <action
            android:id="@+id/action_eventListingFragment_to_postLoginFragment"
            app:destination="@id/post_login_bottom_navigation"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
    </fragment>
    <dialog
