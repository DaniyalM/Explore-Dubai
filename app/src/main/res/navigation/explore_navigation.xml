<!--        app:exitAnim="@anim/fade_out"-->
<!--        app:popEnterAnim="@anim/fade_in"-->
<!--        app:popExitAnim="@anim/fade_out"-->
        <action
            android:id="@+id/action_exploreFragment_to_attractionDetailFragment"
            app:destination="@id/attraction_detail_navigation"
        />
        <action
            android:id="@+id/action_exploreFragment_to_eventDetailFragment2"
            app:destination="@id/event_detail_navigation"
            app:enterAnim="@anim/fade_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/fade_out" />
        <action
            android:id="@+id/action_exploreFragment_to_attractionsFragment"
            app:destination="@id/attractionsFragment"
            app:enterAnim="@anim/fade_in"
